{
  "Version": "2012-10-17",
  "Statement": [
    {
      "Action": "logs:*",
      "Resource": "*",
      "Effect": "Allow"
    },
    {
      "Action": "ec2:*",
      "Resource": "*",
      "Effect": "Allow"
    },
    {
      "Effect": "Allow",
      "Action": "lambda:*",
      "Resource": "*"
    },
    {
      "Effect": "Allow",
      "Action": "events:*",
      "Resource": "*"
    },
    {
      "Action": "s3:*",
      "Resource": "*",
      "Effect": "Allow"
    },
    {
      "Action": "dynamodb:*",
      "Resource": "*",
      "Effect": "Allow"
    },
    {
      "Action": "sqs:*",
      "Resource": "*",
      "Effect": "Allow"
    },
    {
      "Action": "iot:*",
      "Resource": "*",
      "Effect": "Allow"
    },
    {
      "Effect": "Allow",
      "Action": "cloudformation:*",
      "Resource": "*"
    },
    {
      "Effect": "Allow",
      "Action": "secretsmanager:GetSecretValue",
      "Resource": "*"
    },
    {
      "Effect": "Allow",
      "Action": [
        "ssm:GetParameter",
        "ssm:DescribeParameters"
      ],
      "Resource": "*"
    },
    {
      "Effect": "Allow",
      "Action": "kms:Decrypt",
      "Resource": "*"
    },
    {
      "Effect": "Allow",
      "Action": "iam:*",
      "Resource": [
        "arn:aws:iam::487740830288:role/denaribot*",
        "arn:aws:iam::487740830288:policy/denaribot*"
      ]
    },
    {
      "Effect": "Allow",
      "Action": "application-autoscaling:*",
      "Resource": "*"
    },
    {
      "Effect": "Allow",
      "Action": "cloudwatch:*",
      "Resource": "*"
    }
  ]
}
